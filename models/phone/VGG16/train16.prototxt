####################  fc0  ####################

layer {
  name: "fc0"
  type: "InnerProduct"
  bottom: "pool4"
  top: "fc0"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 1024
    weight_filler { type: "gaussian" std: 0.005 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name: "relu0"
  type: "ReLU"
  bottom: "fc0"
  top: "fc0"
}
layer {
  name: "drop0"
  type: "Dropout"
  bottom: "fc0"
  top: "fc0"
  dropout_param {
    dropout_ratio: 0.5
  }
}


layer {
  name: "score0"
  type: "InnerProduct"
  bottom: "fc0"
  top: "score0"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 10
    weight_filler { type: "gaussian" std: 0.005 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name: "loss0"
  type: "SoftmaxWithLoss"
  bottom: "score0"
  bottom: "label0"
  propagate_down: 1
  propagate_down: 0
  top: "loss0"
  loss_weight: 1
  loss_param {
    ignore_label: -1
  }
}



